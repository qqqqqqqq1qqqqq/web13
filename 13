//1
function unique(arr) {
  return new Set(arr)
}

console.log(Array.from(unique([1, 2, 2, 3, 4, 4, 5, 5])))

//2
function aclean(arr) {
  let map = new Map();

  for (let word of arr) {
    let sorted = word.toLowerCase().split("").sort().join("");
    map.set(sorted, word);
  }

  return Array.from(map.values());
}

let arr = ["nap", "teachers", "cheaters", "PAN", "ear", "era", "hectares"];

console.log(aclean(arr))
